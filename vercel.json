{
  "functions": {
    "api/dynamic-routes.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/decks/([^/]+)",
      "destination": "/api/dynamic-routes?model=decks&id=$1"
    },
    {
      "source": "/api/readings/([^/]+)",
      "destination": "/api/dynamic-routes?model=readings&id=$1"
    },
    {
      "source": "/api/querents/([^/]+)",
      "destination": "/api/dynamic-routes?model=querents&id=$1"
    },
    {
      "source": "/api/tags/([^/]+)",
      "destination": "/api/dynamic-routes?model=tags&id=$1"
    },
    {
      "source": "/api/spreads/([^/]+)",
      "destination": "/api/dynamic-routes?model=spreads&id=$1"
    }
  ]
}